<?xml version="1.0" ?>
<DialogList>
  <Dialog>
  <!-- Attributes -->
    <id>1</id>
    <triggerLocation>
      <x1>469</x1>
      <x2>769</x2>
      <y1>476</y1>
      <y2>486</y2>
    </triggerLocation>
    <triggerObject></triggerObject>
    <useItem></useItem>
    <pickItem></pickItem>
    <triggerState></triggerState>
    <transitToState></transitToState>
    <!-- Texts -->
    <textList>
      <text speaker="Me">What’s going on? My head feels like it's imploding.</text>
      <text speaker="Me">It’s been years since I was here last. It looks different.</text>
      <text speaker="Narrator">You notice the clock on your wrist, the countdown has started.</text>
    </textList>
  </Dialog>

  <Dialog>
  <!-- Attributes -->
    <id>2</id>
    <triggerLocation></triggerLocation>
    <triggerObject>table</triggerObject>
    <useItem></useItem>
    <pickItem></pickItem>
    <triggerState></triggerState>
    <transitToState></transitToState>
    <!-- Texts -->
    <textList>
      <text speaker="Me">There's a note written in blood on the table. </text>
      <text speaker="Narrator">The note says, A puppet master who pays no mind to its puppets is a swindler, </text>
      <text speaker="Narrator">takes and takes but never gives, he is much like yourself, useless and greedy. </text>
      <text speaker="Me">Is this a riddle of some sort? This is so sick man. What does this even mean?! </text>
    </textList>
   </Dialog>

